<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MainActivity"
    android:orientation="vertical">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Initialize SDK"
        android:id="@+id/initialize_sdk"
        android:onClick="onClickInitializeSdk" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Custom Event"
        android:id="@+id/button2"
        android:onClick="onClickCustom" />


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/save_custom_preferences"
        android:id="@+id/buttonSaveCustomPreference"
        android:onClick="onClickSaveCustomPreference"
         />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/load_custom_preferences"
        android:id="@+id/buttonLoadCustomPreference"
        android:onClick="onclickPrintCustomPreference"
        />

    <Button
        android:id="@+id/toggle_sdk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClickToggleSDK"
        android:text="@string/disable_sdk_button_str" />

    <TextView
        android:id="@+id/textViewEvents"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    </TextView>

</LinearLayout>
